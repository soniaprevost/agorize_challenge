<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <div class="text-center small-padded">
        <%= link_to '<i class="fa fa-home"></i>'.html_safe, root_path %>
      </div>
    </div>
    <div class="col-sm-12">
      <div class="text-center" id="box">
        <% if @document.box_view_id.present? %>
          <div class="embed-responsive embed-responsive-4by3">
            <iframe class="embed-responsive-item" src="<%= @document.box_view_id %>" frameborder="0" allowfullscreen="allowfullscreen"></iframe>
          </div>
        <% else %>
          # <% @document.box_viewer %>
          <p>Your document is loading... Refresh the page</p>
          <div class="box_loader">
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>